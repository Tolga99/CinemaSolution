<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TEST_WPF"
        xmlns:ViewModels="clr-namespace:TEST_WPF.ViewModels" x:Class="TEST_WPF.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.DataContext>
        <ViewModels:PersonModel/>
    </Window.DataContext>
    <Grid>
        <Label Content="{Binding testModel}"></Label>
        <TextBlock Text="{Binding testModel}"></TextBlock>
    </Grid>
</Window>
